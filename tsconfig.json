{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "sourceMap": true,
    "target": "es6",
    "noImplicitAny": false,
    "strict": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "types": [
      "quasar",
      "node"
    ]
  },
  "exclude": ["node_modules", "research-catch-server"]
}
